<script>
    $(function (){
   $('.content-body').imagesLoaded(function(){
            $('.content-body').masonry({
                itemSelector: '.item'
            });
        });
     })


</script>
<script>pangu.spacingPage()</script>
<script>  
    const gitalk = new Gitalk({
        clientID: '<%=theme.github.gitment.oauth.client_id%>',
        clientSecret: '<%=theme.github.gitment.oauth.client_secret%>',
        repo: '<%=theme.github.gitment.repo%>',
        owner: '<%=theme.github.gitment.owner%>',
        admin: ['<%=theme.github.gitment.owner%>'],
        id: md5(decodeURI(location.pathname)),      // Ensure uniqueness and length less than 50
        distractionFreeMode: false  // Facebook-like distraction free mode
    })

    gitalk.render('comments')
    
</script>

